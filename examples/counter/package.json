{
  "name": "module-i18n-counter-example",
  "version": "0.0.1",
  "description": "module i18n counter example",
  "main": "lib/server/index.jsx",
  "scripts": {
    "start": "webpack-dev-server --config ./tools/webpack.config.dev.js",
    "start:server": "BABEL_ENV=server babel-node src/server",
    "clean:server": "rm -rf lib",
    "clean:client": "rm -rf dist",
    "build:client": "npm run clean:client && webpack --config ./tools/webpack.config.client.js",
    "build:server": "npm run clean:server && BABEL_ENV=server babel src --copy-files --out-dir ./lib",
    "build": "npm run build:client && npm run build:server"
  },
  "author": "Kenzhaev Artur",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "express": "^4.14.1",
    "module-i18n": "0.0.4",
    "node-sass": "^4.1.1",
    "pug": "^2.0.0-beta6",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-css-modules-transform": "^1.1.0",
    "babel-plugin-transform-es2015-template-literals": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-airbnb": "^2.1.1",
    "babel-preset-es2015-without-strict": "0.0.4",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-root-import": "^4.1.5",
    "css-loader": "^0.26.1",
    "eslint-loader": "^1.6.1",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "html-webpack-plugin": "^2.26.0",
    "node-sass": "^4.4.0",
    "pug-loader": "^2.3.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.0-rc.2",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^2.2.0-rc.0",
    "webpack-hot-middleware": "^2.15.0"
  }
}
